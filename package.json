{
  "name": "zustand-dev-tools",
  "version": "0.1.0",
  "license": "MIT",
  "author": "joshwrn",
  "main": "dist/index.js",
  "module": "dist/zustand-dev-tools.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 81,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/zustand-dev-tools.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/zustand-dev-tools.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@tsconfig/recommended": "^1.0.7",
    "@tsconfig/vite-react": "^3.0.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-portal": "^4.0.4",
    "@types/styled-components": "^5.1.26",
    "dts-cli": "^2.0.5",
    "husky": "^9.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.77.8",
    "size-limit": "^11.1.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "framer-motion": "^7.3.2",
    "immer": "^10.1.1",
    "react-draggable": "^4.4.6",
    "react-icons": "^4.4.0",
    "react-portal": "^4.2.2",
    "styled-components": "^5.3.5",
    "zustand": "^4.4.7"
  }
}
